{
  "name": "apinni-ts",
  "version": "1.0.0",
  "description": "Type-safe API client generator with decorators for TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "bin": {
    "apinni": "dist/cli.js"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup && chmod +x dist/cli.js dist/cli.mjs",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "typescript",
    "api",
    "client",
    "generator",
    "decorators",
    "type-safe"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "bundle-require": "^5.1.0",
    "chokidar": "^4.0.3",
    "commander": "^14.0.0",
    "listr2": "^9.0.1",
    "swagger-express-ts": "^1.1.0",
    "ts-morph": "^26.0.0"
  },
  "peerDependencies": {
    "reflect-metadata": "^0.2.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.0.3",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-plugin-prettier": "^5.5.3",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.38.0",
    "vitest": "^3.2.4"
  },
  "license": "MIT"
}
